<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.4" name="Java DDD">
    <description>Provides shared rules for the jQAssistant DDD Plugin.</description>
    <model>
        <class>org.jqassistant.contrib.plugin.ddd.descriptor.DDDDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ddd.descriptor.BoundedContextDescriptor</class>
        <class>org.jqassistant.contrib.plugin.ddd.descriptor.LayerDescriptor</class>
    </model>
    <rules>
        <resource>java-ddd.xml</resource>
    </rules>
</jqa-plugin:jqassistant-plugin>
